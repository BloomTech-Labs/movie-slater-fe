{
    "version": 2,
    "name": "movie-knight",
    "runs": [
        {
            "src": "movie-knight/package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "coverage"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "movie-knight/$1"
        }
    ]
}